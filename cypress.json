{
  "baseUrl": "https://example.cypress.io/todo#",
  "testFiles": ["**/*.feature"],
  "reporter": "mochawesome",
  "reporterOptions": {
    "reporterEnabled": "mochawesome",
    "mochawesomeReporterOptions": {
      "reporterDir": "cypress/report/mochawesome-report",
      "quite": true,
      "overwrite": true,
      "html": true,
      "json": false,
      "timestamp": "mmddyyyy_HHMMss"
    }
  }
}
